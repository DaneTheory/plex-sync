    "caporal": "^0.5.0",
    "chalk": "^1.1.3",
    "conf": "^0.12.0",
    "dotenv": "^4.0.0",
    "inquirer": "^3.0.6",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.8.3",
    "ora": "^1.2.0",
    "query-string": "^4.3.4",
    "ts-progress": "^0.1.2",
    "update-notifier": "^2.1.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
