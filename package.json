    "sync": "node lib/index.js",
    "test": "npm run build",
    "watch": "npm run build -- --watch"
  },
  "author": "Jacob Gillespie <jacobwgillespie@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.1",
