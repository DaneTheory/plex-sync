{
  "name": "plex-sync",
  "version": "0.6.1",
  "description": "Sync watched status between Plex servers",
  "main": "lib/index.js",
  "bin": {
    "plex-sync": "bin/plex-sync"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "babel src --out-dir lib/",
